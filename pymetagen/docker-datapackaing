 GLENPARAMS=test_uae_banking # Name for the job - preferably a timestamp/uuid
PRODUCT=uaeBanking
GLENPARAMSDIRNAME="input_${GLENPARAMS}" # Unique directory name for the job

echo "GLENPARAMS: $GLENPARAMS"

# Produce raw data with uncannyglen
docker run \
    -v "./glenparams:/app/uncannyglen/glenparams" \
    -v ./:/app/glenresults \
    -e "GLENPARAMS=${GLENPARAMS}" \
    -e "GLENLOGPROGRESS=TEXT" \
    -e "GLENLOGLEVEL=INFO" \
    -e "GLENPARAMSDIR=/app/uncannyglen/glenparams/${GLENPARAMSDIRNAME}" \
    --rm -it 121483253002.dkr.ecr.eu-west-2.amazonaws.com/uncannyglen:4.0.4

# # Produce refined data with datapackaging
docker run \
    -v ./:/app/glenresults \
    -e "SDF=/app" \
    -e "GLENPARAMS=${GLENPARAMS}" \
    -e "PRODUCT=${PRODUCT}" \
    --rm -it 121483253002.dkr.ecr.eu-west-2.amazonaws.com/datapackaging:4.0.3.2 \
    all zip